<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <%- seo.title %>
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta http-equiv='Content-Language' content='en' />
    <%- seo.meta %>
      <link rel="icon" href="/images/favicon.ico" type="image/x-icon" />
      <meta name="viewport" content="width=device-width,initial-scale=1.0">
      <%- commonCss %>
        <%- pageCss %>
          <style>
            .slideout-menu {
              display: none;
            }
          </style>
          <!-- Facebook Pixel Code -->
          <script>
            !function (f, b, e, v, n, t, s) {
              if (f.fbq) return; n = f.fbq = function () {
                n.callMethod ?
                  n.callMethod.apply(n, arguments) : n.queue.push(arguments)
              }; if (!f._fbq) f._fbq = n;
              n.push = n; n.loaded = !0; n.version = '2.0'; n.queue = []; t = b.createElement(e); t.async = !0;
              t.src = v; s = b.getElementsByTagName(e)[0]; s.parentNode.insertBefore(t, s)
            }(window,
              document, 'script', 'https://connect.facebook.net/en_US/fbevents.js');
            fbq('init', '1911640882445366'); // Insert your pixel ID here.
            fbq('track', 'PageView');
          </script>
          <noscript><img height="1" width="1" style="display:none" src="https://www.facebook.com/tr?id=1911640882445366&ev=PageView&noscript=1"
            /></noscript>
          <!-- DO NOT MODIFY -->
          <!-- End Facebook Pixel Code -->
</head>
<body>
  <script>
    window.fbAsyncInit = function () {
      FB.init({
        appId: '1738998542989048',
        xfbml: true,
        version: 'v2.9'
      });
      FB.AppEvents.logPageView();
    };

    (function (d, s, id) {
      var js, fjs = d.getElementsByTagName(s)[0];
      if (d.getElementById(id)) { return; }
      js = d.createElement(s); js.id = id;
      js.src = "//connect.facebook.net/en_US/sdk.js";
      fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
  </script>
  <div id="swipeMenu" class="slideout-menu"></div>
  <div id="app" class="global-wrapper">
    <%- markup %>
  </div>
  <script>
    window.PRELOADED_STATE = '<%= store %>';
  </script>
  
  <%- metriks %>

  <script type="text/javascript" src="/js/jQuery_1.11.min.js" defer></script>
  <script type="text/javascript" src="/js/redactor.min.js?v=2.6" defer></script>
  <script type="text/javascript" src="/js/source.js" defer></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/slideout/1.0.1/slideout.min.js" defer></script>
  <script src="<%= baseUrl %><%= jsUrl %>" defer></script>
  <link href="https://fonts.googleapis.com/css?family=Bitter" rel="stylesheet">
  <link rel="stylesheet" href="<%= baseUrl %><%= cssUrl %>">
</body>

</html>