<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <%- seo.title %>
    <meta http-equiv='X-UA-Compatible' content='IE=edge' />
    <meta http-equiv='Content-Language' content='en' />
    <%- seo.meta %>
      <link rel="icon" href="/images/favicon.ico" type="image/x-icon" />
      <meta name="viewport" content="width=device-width,initial-scale=1.0">
      <%- commonCss %>
      <%- pageCss %>
        <style>
          .slideout-menu {
            display: none;
          }
        </style>
</head>

<body>
  <script>
    window.fbAsyncInit = function () {
      FB.init({
        appId: '1738998542989048',
        xfbml: true,
        version: 'v2.9'
      });
      FB.AppEvents.logPageView();
    };

    (function (d, s, id) {
      var js, fjs = d.getElementsByTagName(s)[0];
      if (d.getElementById(id)) { return; }
      js = d.createElement(s); js.id = id;
      js.src = "//connect.facebook.net/en_US/sdk.js";
      fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));

  </script>
  <div id="swipeMenu" class="slideout-menu"></div>
  <div id="app" class="global-wrapper">
    <%- markup %>
  </div>
  <script>
    window.PRELOADED_STATE = '<%= store %>';
  </script>
  <script>
    (function (i, s, o, g, r, a, m) {
      i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
        (i[r].q = i[r].q || []).push(arguments)
      }, i[r].l = 1 * new Date(); a = s.createElement(o),
        m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
    })(window, document, 'script', 'https://www.google-analytics.com/analytics.js', 'ga');

    ga('create', 'UA-78577116-1', 'auto');
    ga('send', 'pageview');
  </script>
  <script type="text/javascript" src="/js/jQuery_1.11.min.js" defer></script>
  <script type="text/javascript" src="/js/redactor.min.js" defer></script>
  <script src="<%= baseUrl %><%= jsUrl %>" defer></script>
</body>

</html>
<link href="https://fonts.googleapis.com/css?family=Bitter" rel="stylesheet">
<link rel="stylesheet" href="<%= baseUrl %><%= cssUrl %>">